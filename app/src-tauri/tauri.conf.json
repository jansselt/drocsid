{
  "productName": "Drocsid",
  "version": "0.2.1",
  "identifier": "com.drocsid.desktop",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devUrl": "http://localhost:5174",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "Drocsid",
        "width": 1280,
        "height": 800,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "createUpdaterArtifacts": true,
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "updater": {
      "pubkey": "RWTMhARV38gYejBYiFJIyiW1l9m85/B0M3Vq1GXIUAnNmSrN2baMlDVz",
      "endpoints": [
        "https://github.com/jansselt/drocsid/releases/latest/download/latest.json"
      ]
    }
  }
}
